    "pinia": "^2.1.7",
    "uniqid": "^5.4.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.18.5",
    "@types/uniqid": "^5.3.3",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/tsconfig": "^0.4.0",
    "concurrently": "^8.2.2",
    "npm-run-all2": "^6.1.1",
    "sass": "^1.69.4",
    "typescript": "~5.2.0",
